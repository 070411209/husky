# Clearpath Husky rosinstall file
# Installs "gold source" from github
- other: {local-name: /opt/ros/electric/ros}
- other: {local-name: /opt/ros/electric/stacks}
- git: {local-name: clearpath_husky, uri: 'https://github.com/clearpathrobotics/clearpath_husky',
    version: master}
- git: {local-name: clearpath_common, uri: 'https://github.com/clearpathrobotics/clearpath_common',
    version: master}
- git: {local-name: husky_apps, uri: 'https://github.com/clearpathrobotics/husky_apps',
    version: master}
- git: {local-name: husky_simulator, uri: 'https://github.com/clearpathrobotics/husky_simulator',
    version: master}
